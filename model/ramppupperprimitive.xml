    <include file="rampscene2.xml"/>

    <default>
      <geom friction="0.2 0.0005 0.00001" condim="6"/>
  </default>

    <option solver="CG" tolerance="1e-6"/>

    <size nconmax="500" njmax="5000" nstack="5000000"/>

    <worldbody>
        <!-- x = width_box * sqrt(2), z = height of ramp = 0.02*sqrt(2) -->
        <!-- quat = "cos(π/8) sin(π/8) 0 0". This corresponds to a 45 degree
            rotation about vecotor [1, 0, 0] (i.e. x-axis) to create a 45 degree ramp-->
        <body name="torso" pos="0 0 0.48" quat="0.96592582628906830 0 0.25881904510252074 0">
        <!-- <body pos="0.0675 0 0.4906854249492381" quat="0.9238795325112867 0 0.3826834323650898 0"> -->
            <freejoint/>
            <!-- <geom type="box" size=".025 .05 .025" quat="0.9238795325112867 0 0.3826834323650898 0" material="matobj"/> -->
            <geom name="blockg" type="box" size="0.15 0.03 0.002" material="matobj"/>
            <body name="fr_module">
                <joint name="fr_x" axis="1 0 0" range="-1.57 1.57"/>
                <body name="fr" pos="0.1 -0.03 0">
                    <!-- <joint name="fr_y" axis="0 1 0" range="-1.57 1.57"/> -->
                    <geom axisangle="0 1 0 0" name="fr" fromto="0 0 0 0 0 -0.125" size="0.01" type="capsule" rgba="0 0 255 0.8"/>
                    <!-- <body name="fr2" pos="0 0 -0.1235"> -->
                        <!-- <joint name="fr_y2" axis="0 1 0" range="-3.14 0.0"/> -->
                        <!-- <geom axisangle="0 1 0 0" name="fr_l2" fromto="0 0 0 0 0 -0.125" size="0.01" type="capsule"/>
                    </body> -->
                </body>
            </body>
            <body name="br" pos="-0.1 -0.04 0">
                <joint name="br_x" axis="1 0 0" range="-1.57 1.57"/>
                <geom axisangle="0 1 0 0" name="br" fromto="0 0 0 0 0 -0.125" size="0.01" type="capsule"  rgba="0 0 255 0.8"/>
            </body>
            <body name="fl" pos="0.1 0.04 0">
                <joint name="fl_x" axis="1 0 0" range="-1.57 1.57"/>
                <geom axisangle="0 1 0 0" name="fl" fromto="0 0 0 0 0 -0.125" size="0.01" type="capsule"  rgba="0 0 255 0.8"/>
            </body>
            <body name="bl" pos="-0.1 0.04 0">
                <joint name="bl_x" axis="1 0 0" range="-1.57 1.57"/>
                <geom axisangle="0 1 0 0" name="bl" fromto="0 0 0 0 0 -0.125" size="0.01" type="capsule"  rgba="0 0 255 0.8"/>
            </body>
            <site name="blocks" type="box" size="0.15 0.03 0.002" rgba="0 255 0 .2"/>

        </body>
    </worldbody>

    <sensor>
        <framelinvel name="gvel" objtype="geom" objname="blockg"/>
        <!-- <velocimeter name="lvel" site="blocks"/> -->
    </sensor>

</mujoco>
